<lxs>
    <scene root="Root" axis_length="5"/>

    <views default="Arena View">
        <perspective id="Arena View" near="1" far="500" angle="50">
            <from x="8" y="10" z="15" />
            <to x="0" y="0" z="0" />
        </perspective>
        <perspective id="Floating View" near="1" far="500" angle="45">
            <from x="15" y="-4" z="15" />
            <to x="0" y="0" z="0" />
        </perspective>
        
        <ortho id="CatView" near="-2" far="50" left="-3" right="3" top="2" bottom="-1">
            <to  x="-1" y="0" z="-3"/>
			<from  x="0" y="1" z="-1"/>
        </ortho>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="OmniLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1" linear="0" quadratic="0"/>

        </omni>
        <spot id="SpotLight1" enabled="1" angle="45" exponent="10">
            <location x="-2" y="1" z="0" w="1.0" />
            <target x="-5" y="2" z="0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1" linear="0" quadratic="0"/>
        </spot>
        <spot id="SpotLight2" enabled="1" angle="45" exponent="10">
            <location x="2" y="1" z="0" w="1.0" />
            <target x="5" y="2" z="0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1" linear="0" quadratic="0"/>
        </spot>
    </lights>

    <textures>

    </textures>

    <materials>
        <material id="NeutralMaterial" shininess="10">
            <emission r="0.8" g="0.8" b="0.8" a="1.0" />
            <ambient r="0.8" g="0.8" b="0.8" a="1.0" />
            <diffuse r="0.1" g="0.1" b="0.1" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </material>
        <material id="goldenMaterial" shininess="10">
            <emission r="0.83" g="0.68" b="0.215" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.83" g="0.68" b="0.215" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>
        <material id="whiteMaterial" shininess="1">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>
        
        <material id="darkerMaterial" shininess="10">
            <emission r="0.4" g="0.4" b="0.4" a="1.0" />
            <ambient r="0.4" g="0.4" b="0.4" a="1.0" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1.0" />
            <specular r="0.4" g="0.4" b="0.4" a="1.0" />
        </material>
        <material id="redMaterial" shininess="10">
            <emission r="1.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="1.0" g="0.0" b="0.0" a="1.0" />
            <specular r="1.0" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="Transform">
            <translate x="4" y="5" z="6" />
            <scale x="1" y="1" z="1" />
            <rotate axis="x" angle="-45" />
            <rotate axis="z" angle="-45" />
        </transformation>
    </transformations>

    <primitives>
        <primitive id="Rectangle">
            <rectangle x1="0" y1="0" x2="1" y2="1" />
        </primitive>
        <primitive id="Board">
            <board x1="-1" y1="0" x2="3" y2="3" />
        </primitive>
        <primitive id="Cylinder">
            <cylinder base="0.1" top="0.1" height="2" slices="20" stacks="5"/>
        </primitive>
        <primitive id="Triangle">
            <triangle x1="0" y1="0" z1="0" x2="1" y2="0" z2 = "0" x3="0" y3="1" z3="0"/>
        </primitive>
        <primitive id="ObliqueTriangle">
            <triangle x1="0" y1="1" z1="-0.5" x2="-0.5" y2="0" z2 = "0" x3="0.5" y3="0" z3="0"/>
        </primitive>
        <primitive id="Sphere">
            <sphere radius="1" slices="20" stacks="20"/>
        </primitive>
        <primitive id="Torus">
            <torus inner="0.05" outer = "0.3" slices="20" loops="20"/>
        </primitive>
        <primitive id="LessDefinedTorus">
            <torus inner="0.03" outer = "0.3" slices="20" loops="10"/>
        </primitive>
        <primitive id="Patch">
             <patch npointsU="3" npointsV="2" npartsU="4" npartsV="4">
                    <controlpoint xx="0.3" yy="0" zz="-0.2" />
                    <controlpoint xx="0.1" yy="0" zz="0.2" />
                    <controlpoint xx="0" yy="0.3" zz="-0.2" />
                    <controlpoint xx="0" yy="0.2" zz="0.2" />
                    <controlpoint xx="-0.3" yy="0" zz="-0.2" />
                    <controlpoint xx="-0.1" yy="0" zz="0.2" />
            </patch>
        </primitive>
        <primitive id="Plane">
            <plane npartsU="30" npartsV="30" />
        </primitive>
        <primitive id="Cylinder2">
            <cylinder2 base="0.1" top="0.1" height="2" slices="50" stacks="50" />
        </primitive>  
    </primitives>

    <animations>
        <animation id="CatchTheSnitch" >
            <keyframe instant="3">
                <translate x="0" y="4" z="7" />
                <rotate angle_x="-45" angle_y="-45" angle_z="20" />
                <scale x="2" y="2" z="2" />
            </keyframe>
            <keyframe instant="5">
                <translate x="-8" y="5" z="7" />
                <rotate angle_x="12" angle_y="-90" angle_z="90" />
                <scale x="2" y="2" z="2" />
            </keyframe> 
             <keyframe instant="7">
                <translate x="-8" y="4" z="0" />
                <rotate angle_x="0" angle_y="-180" angle_z="120" />
                <scale x="1" y="1" z="1" />
            </keyframe>
       
            <keyframe instant="12">
                <translate x="-8" y="3" z="-3" />
                <rotate angle_x="0" angle_y="90" angle_z="0" />
                <scale x="1" y="1" z="1" />

            </keyframe>
            <keyframe instant="17">
                <translate x="0" y="3" z="-5" />
                <rotate angle_x="45" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="19">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>

        </animation>
    </animations>

    <components>
        <component id="Board1">
            <transformation>
                
            </transformation>
            <materials>
                <material id="darkerMaterial" />
                <material id="whiteMaterial" />
                <material id="redMaterial" />
            </materials>
            <texture id="none" length_s="1" length_t="1"/>
            <children>
                <primitiveref id="Board" />
            </children>
        </component>

     

        <component id="Root">
            <transformation> 
            </transformation>
            <materials>
                <material id="NeutralMaterial" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="Board1" />

            </children>
        </component>
    </components>
</lxs>